<ExtensionManifest Version="7.0" ExtensionBundleId="com.youtubedownloader.premiere" ExtensionBundleVersion="2.6.1"
                   ExtensionBundleName="YouTube Downloader" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Author>
    <![CDATA[YouTube Downloader Extension]]>
  </Author>
  <Contact mailto="support@example.com"/>
  <Legal>
    <![CDATA[For personal use only. Ensure compliance with YouTube Terms of Service.]]>
  </Legal>
  <Abstract href="https://example.com">
    <![CDATA[Download YouTube videos directly into Premiere Pro with customizable options.]]>
  </Abstract>
  <ExtensionList>
    <Extension Id="com.youtubedownloader.premiere.panel" Version="2.6.1"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PPRO" Version="[25.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="11.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.youtubedownloader.premiere.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>YouTube Downloader</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>400</Width>
            </Size>
            <MinSize>
              <Height>400</Height>
              <Width>350</Width>
            </MinSize>
            <MaxSize>
              <Height>800</Height>
              <Width>600</Width>
            </MaxSize>
          </Geometry>
          <Icons>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
